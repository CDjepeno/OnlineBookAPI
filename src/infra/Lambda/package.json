{
  "name": "lambda",
  "version": "0.1.0",
  "bin": {
    "lambda": "bin/lambda.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "start": "ts-node ./clients/aurora.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "typeorm:migrate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "typeorm:run": "npm run typeorm migration:run",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:entity": "npm run typeorm entity:create -n",
    "migrate:up": "ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d ormconfig.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@types/glob": "^8.0.0",
    "@types/jest": "^27.5.2",
    "@types/node": "10.17.27",
    "@types/prettier": "2.6.0",
    "aws-cdk": "2.44.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "aws-cdk-lib": "^2.87.0",
    "babel-jest": "^29.2.1",
    "constructs": "^10.1.137",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.12",
    "glob": "^8.0.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "path": "^0.12.7",
    "source-map-support": "^0.5.21",
    "typeorm": "^0.3.10"
  }
}
